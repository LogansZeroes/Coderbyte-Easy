function ABCheck(str) { 

  for (var i =0; i < str.length-3; i++) {
    if (str[i].toLowerCase() == "a" && str[i+4].toLowerCase() == "b") {
      return true;
    }
    else if (str[i].toLowerCase() == "b" && str[i+4].toLowerCase() == "a") {
      return true;
    }
  }
  // code goes here  
  return false; 
         
}
