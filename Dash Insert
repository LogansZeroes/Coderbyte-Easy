function DashInsert(str) { 
  var ans = "";
  str = str.toString();
  
  for (var x = 0; x < str.length - 1; x++) {
    ans += str.charAt(x);
    
    if ( str.charAt(x)%2 != 0 && str.charAt(x) != 0 && str.charAt(x+1)%2 != 0 && str.charAt(x+1) != 0) {
      ans += '-';
    }
    
  }
  ans += str.charAt(str.length - 1);
  // code goes here  
  return ans; 
         
}
